{{#categories}}
  <div id="outline-container-sec-{{id}}" class="outline-2 post index">
    <h2 id="sec-{{id}}">{{category}}</h2>
    <div class="outline-text-2" id="text-{{id}}">
        <ul class="org-ul">
        {{#posts}}
            <li><a href="{{post-uri}}"><aside>{{post-date}}</aside>{{post-title}}</a></li>
            {{post-content}}
        {{/posts}}
      </ul>
    </div>
  </div>
{{/categories}}
